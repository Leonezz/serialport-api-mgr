{
  "productName": "serialport-api",
  "version": "0.1.0",
  "identifier": "com.serialport-api.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "serialport-api",
        "minWidth": 1080,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
